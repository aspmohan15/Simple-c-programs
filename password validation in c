#include<stdio.h>
#include<conio.h>
int usern(int);
int pass(int);
void main()
{
 int u,p;
    printf("enter user name");
    scanf("%d",&u);
    usern(u);
    printf("Enter password");
    scanf("%d",&p);
    pass(p);
    
    getch();
}
int usern(int u)
{
    if(u==1234)
    {
    printf("suceesfull\n");
    return 1;
    }
    else
    {
    printf("invalid password\n");
    }
}
int pass(int p)
{
    if(p==1234)
    {
        printf("correct\n");
    }
    else
    {
        printf("Invalid password\n");
    }

    
}
